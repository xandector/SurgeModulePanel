#!name=Panels
#!desc=信息面板

[Panel]
#Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=1

#流量统计
TrafficStatistics = script-name=TrafficStatistics,update-interval=1

#网络详情
NET_info = script-name=NET_info,update-interval=1

#Netflix策略控制器
NetflixController = script-name=NetflixController, update-interval=600 

#Nixitally信息
Sub_info = script-name=Sub_info_1,update-interval = 43200

#FlowerCloud信息
Sub_info = script-name=Sub_info_2,update-interval = 43200

#薯条信息
Sub_info = script-name=Sub_info_3,update-interval = 43200

#策略组面板 可重复配置 注意修改相应字段
groupPanel= script-name=groupPanel01,update-interval=5

groupPanel= script-name=groupPanel02,update-interval=5

groupPanel= script-name=groupPanel03,update-interval=5

groupPanel= script-name=groupPanel04,update-interval=5

groupPanel= script-name=groupPanel05,update-interval=5

groupPanel= script-name=groupPanel06,update-interval=5

groupPanel= script-name=groupPanel07,update-interval=5

groupPanel= script-name=groupPanel08,update-interval=5

[Script]
#附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js ,argument=icon=crown.fill&color=#f6c970

#流量统计 点击以切换网络界面
TrafficStatistics = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js ,argument=icon=arrow.up.arrow.down.circle&color=#5d84f8

#網路詳情 标题显示为根节点名
#应当修改的字段 group 代理策略组名称
NET_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/net_info.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=🚀 节点选择

#应该修改的字段 netflixGroup
NetflixController = type=generic, script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/netflixcontroller.js, timeout=3600, argument=icon1=checkmark.circle&color1=55ba94&icon2=checkmark.circle.trianglebadge.exclamationmark&color2=#9a9ced&icon3=hand.raised.circle&color3=#ea5532&netflixGroup=🎥 奈飞视频
NetflixChecker = type=cron,cronexp=10 * * * *,wake-system=0,timeout=3600,script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/netflixchecker.js,script-update-interval=86400,control-api=1
#捷径执行
NetflixShortcut = type=cron,cronexp=5 * 31 1 2000,wake-system=0,timeout=3600,script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/netflixshortcut.js,script-update-interval=86400,control-api=1

#Nixitally信息
#必须修改的字段:你encode后的机场订阅链接
Sub_info_1 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js ,script-update-interval=0,argument=url=https%3A%2F%2Fnaixisubs.com%2FdownloadConfig%2FShadowRocketImportService.aspx%3Ft%3Dssn%26urk%3De224e991-b95f-4342-bc90-24164d03d6c9&reset_day=15&title=Nixitally&icon=n.circle.fill&color=#5AC8FA

#FlowerCloud信息
#必须修改的字段:你encode后的机场订阅链接
Sub_info_2 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js ,script-update-interval=0,argument=url=https%3A%2F%2Fapi.flowercloud.xyz%2Fosubscribe.php%3Fsid%3D4903%26token%3DCbQ6F76mEYEQ%26ssr%3D1&reset_day=5&title=FlowerCloud&icon=leaf.circle.fill&color=#5AC8FA

#薯条信息
#必须修改的字段:你encode后的机场订阅链接
Sub_info_3 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js ,script-update-interval=0,argument=url=https%3A%2F%2Ffast.losadhwselfff2332dasd.xyz%2Flink%2F3mrKuUjXDq7F8EZt%3Fsub%3D1&title=CNIX&icon=antenna.radiowaves.left.and.right.circle.fill&color=#5AC8FA


#策略组面板 可重复配置 注意修改相应字段
